{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "CustomLogger": {
      "Options": {
        "FolderPath": "",
        "FilePath": "log_{date}.log"
      },
      "LogLevel": {
        "CustomLogger.HostedServices.Web.Controllers": "Information",
        "Custom.HostedServices.Web.HostedServices": "Information"
      }
    }
  },
  "AllowedHosts": "*"
}
